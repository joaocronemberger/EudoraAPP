unit dtoCliente;

interface

type
  TCliente = class
  private
    FNome: String;
    FTelefone: String;
    FFacebook: String;
    public
      property Nome: String read FNome write FNome;
      property Telefone: String read FTelefone write FTelefone;
      property Facebook: String read FFacebook write FFacebook;
  end;

implementation

end.
