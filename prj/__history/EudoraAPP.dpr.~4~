program EudoraAPP;

uses
  Vcl.Forms,
  viewPrincipal in '..\view\viewPrincipal.pas' {vwPrincipal},
  dtoCliente in '..\dto\dtoCliente.pas',
  dtoVenda in '..\dto\dtoVenda.pas',
